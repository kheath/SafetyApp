<?xml version="1.0" encoding="UTF-8"?>
<project name="module_safetyapp" default="compile.module.safetyapp">
  <dirname property="module.safetyapp.basedir" file="${ant.file.module_safetyapp}"/>
  
  <property name="module.jdk.home.safetyapp" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.safetyapp" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.safetyapp" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.safetyapp" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="safetyapp.output.dir" value="undefined"/>
  <property name="safetyapp.testoutput.dir" value="undefined"/>
  
  <path id="safetyapp.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="safetyapp.module.production.classpath"/>
  
  <path id="safetyapp.runtime.production.module.classpath"/>
  
  <path id="safetyapp.module.classpath"/>
  
  <path id="safetyapp.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.safetyapp">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.safetyapp">
    <patternset refid="excluded.from.module.safetyapp"/>
  </patternset>
  
  
  <target name="compile.module.safetyapp" depends="compile.module.safetyapp.production,compile.module.safetyapp.tests" description="Compile module SafetyApp"/>
  
  <target name="compile.module.safetyapp.production" depends="register.custom.compilers" description="Compile module SafetyApp; production classes"/>
  
  <target name="compile.module.safetyapp.tests" depends="register.custom.compilers,compile.module.safetyapp.production" description="compile module SafetyApp; test classes" unless="skip.tests"/>
  
  <target name="clean.module.safetyapp" description="cleanup module">
    <delete dir="${safetyapp.output.dir}"/>
    <delete dir="${safetyapp.testoutput.dir}"/>
  </target>
</project>